<?xml version="1.0"?><!DOCTYPE import-control PUBLIC
    "-//Puppy Crawl//DTD Import Control 1.4//EN"
    "http://checkstyle.sourceforge.net/dtds/import_control_1_4.dtd">

<!--suppress XmlHighlighting -->
<import-control pkg="de.tuberlin.mcc.simra.app" strategyOnMismatch="allowed">
    <disallow pkg="de.tuberlin.mcc.simra.app.update" />

    <subpackage name="update">
        <allow class="de.tuberlin.mcc.simra.app.R" />
        <disallow pkg="de.tuberlin.mcc.simra.app" />
    </subpackage>

    <file name="UpdateHelper">
        <allow pkg="de.tuberlin.mcc.simra.app.update" />
    </file>
</import-control>